      "StudentGradeModel": {
        "type": "object",
        "properties": {
          "studentName": {
            "type": "string",
            "nullable": true
          },
          "studentNumber": {
            "type": "string",
            "nullable": true
          },
          "subject": {
            "type": "string",
            "nullable": true
          },
          "grade": {
            "type": "string",
            "nullable": true
          },
          "remark": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
